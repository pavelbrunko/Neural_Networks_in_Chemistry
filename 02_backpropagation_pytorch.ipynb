   "source": [
    "# Backwards\n",
    "import torch\n",
    "x = torch.tensor(1.0)\n",
    "y = torch.tensor(2.0)\n",
    "w = torch.tensor(1.0, requires_grad=True)\n",
    "\n",
    "# forward pass and comput the loss\n",
    "y_hat = w * x\n",
    "loss = (y_hat - y) ** 2\n",
    "print(loss)\n",
    "# backward pass\n",
    "loss.backward()\n",
    "print(w.grad)\n",
    "### update weights\n",
    "### next pass"
   ]
  }
